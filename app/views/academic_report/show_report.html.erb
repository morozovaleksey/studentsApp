<!-- script references -->
<script id="grid-template" type="text/template">
  <table class="table" id="all-students">
    <thead>
    <tr>
      <th class="text-center">ID</th>
      <th>Фамилия</th>
      <th>Имя</th>
      <th>Группа</th>
      <th>Email</th>
      <th>Ip адресс</th>
      <% @subjects.each do |subject| %>
          <th> <%= subject[:subject_name] %> </th>
      <% end %>
      <th>Средний бал</th>

    </tr>
    </thead>
    <tbody id="test-students">

    </tbody>
  </table>

</script>

